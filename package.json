{
  "name": "movie_awards_game_server",
  "version": "1.0.0",
  "description": "A server to manage Movie Awards game redux states",
  "main": "index.js",
  "scripts": {
    "start": "bable-node index.js",
    "test": "node scripts/test.js --env=jsdom --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raziell74/movie_awards_game_server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raziell74/movie_awards_game_server/issues"
  },
  "homepage": "https://github.com/raziell74/movie_awards_game_server#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^22.4.3",
    "raf": "^3.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "redux": "^3.7.2",
    "socket.io": "^2.0.4"
  }
}
